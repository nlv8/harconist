{
  "name": "harconist",
  "displayName": "harconist",
  "version": "0.1.0",
  "description": "Harcon extension for Visual Studio Code.",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc  -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nlv8/harconist.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:harconist.reload"
  ],
  "contributes": {
    "commands": [
      {
        "command": "harconist.reload",
        "title": "Harconist: Reload the typings of the available Harcon entities."
      }
    ]
  },
  "keywords": [],
  "author": "Attila Bagossy",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nlv8/harconist/issues"
  },
  "homepage": "https://github.com/nlv8/harconist#readme",
  "engines": {
    "vscode": "^1.34.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "@types/vscode": "^1.32.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "jsdoc-api": "^5.0.1"
  }
}
